use testDB;

select *
        from phone
        where phone.name = 'Василий'

CREATE INDEX name_index ON phone (name)



testDB> select *
                from phone
                where phone.name = 'Василий'
[2022-02-23 15:35:00] 1 row retrieved starting from 1 in 176 ms (execution: 169 ms, fetching: 7 ms)




//With index
testDB> select *
                 from phone
                 where phone.name = 'Василий'
